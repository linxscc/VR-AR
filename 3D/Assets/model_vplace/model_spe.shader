                float4 _gloss_var = tex2D(_gloss,TRANSFORM_TEX(i.uv0, _gloss));
                float roughness = 1.0 - (_gloss_var.r*_Gloss);
                o.Albedo = diffColor + specColor * roughness * roughness * 0.5;
                
                return UnityMetaFragment( o );
            }
            ENDCG
        }
    }
